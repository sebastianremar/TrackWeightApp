{
  "cli": {
    "version": ">= 14.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": { "simulator": false }
    },
    "preview": {
      "distribution": "store",
      "autoIncrement": true,
      "channel": "preview"
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "<YOUR_APPLE_ID_EMAIL>",
        "ascAppId": "<APP_STORE_CONNECT_APP_ID>",
        "appleTeamId": "<YOUR_TEAM_ID>"
      }
    }
  }
}
